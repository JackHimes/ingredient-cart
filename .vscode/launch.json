{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "Launch Dev",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "preLaunchTask": "npm: build",
      "command": "npm run dev",
      "outFiles": ["${workspaceFolder}/build/**/*.js"],
      "sourceMaps": true,
    }
  ]
}